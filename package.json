{
  "name": "ethical-json-tools",
  "displayName": "Ethical JSON Tools - Secure and Simple",
  "description": "A secure, simple, and transparent set of tools for all your JSON needs. Validate, format, minify, stringify, and unstringify JSON with confidence, knowing your data stays private. No tracking, no telemetry, just pure functionality.",
  "publisher": "AnkitMalikTools",
  "icon": "icon.png",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.91.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ankitmalikg2/ethical-json-tools.git"
  },
  "bugs": {
    "url": "https://github.com/ankitmalikg2/ethical-json-tools/issues"
  },
  "categories": [
    "Formatters",
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "json",
    "formatter",
    "validator",
    "linter",
    "minify",
    "uglify",
    "compress",
    "prettify",
    "beautify",
    "stringify",
    "unescape",
    "escape",
    "tools",
    "secure",
    "ethical",
    "privacy"
  ],
  "activationEvents": [
    "onCommand:ethical-json-tools.validate",
    "onCommand:ethical-json-tools.prettify",
    "onCommand:ethical-json-tools.minify",
    "onCommand:ethical-json-tools.stringify",
    "onCommand:ethical-json-tools.unstringify"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ethical-json-tools.validate",
        "title": "JSON: validate"
      },
      {
        "command": "ethical-json-tools.prettify",
        "title": "JSON: prettify"
      },
      {
        "command": "ethical-json-tools.minify",
        "title": "JSON: minify"
      },
      {
        "command": "ethical-json-tools.stringify",
        "title": "JSON: stringify"
      },
      {
        "command": "ethical-json-tools.unstringify",
        "title": "JSON: unstringify"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "jsonTools",
          "name": "JSON Tools",
          "icon": "json"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "jsonTools",
        "contents": "[Validate](command:ethical-json-tools.validate) \n [Prettify](command:ethical-json-tools.prettify) \n [Minify](command:ethical-json-tools.minify) \n[Stringify](command:ethical-json-tools.stringify) \n[Unstringify](command:ethical-json-tools.unstringify)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}