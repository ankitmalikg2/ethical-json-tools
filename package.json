{
  "name": "ethical-json-tools",
  "displayName": "Ethical Json Tools",
  "description": "this tool has various ethical tools related to Json",
  "publisher": "AnkitMalikTools",
  "icon": "./icon.png",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ethical-json-tools.validate",
    "onCommand:ethical-json-tools.prettify",
    "onCommand:ethical-json-tools.minify",
    "onCommand:ethical-json-tools.stringify",
    "onCommand:ethical-json-tools.unstringify"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ethical-json-tools.validate",
        "title": "Validate"
      },
      {
        "command": "ethical-json-tools.prettify",
        "title": "Prettify"
      },
      {
        "command": "ethical-json-tools.minify",
        "title": "Minify"
      },
      {
        "command": "ethical-json-tools.stringify",
        "title": "Stringify"
      },
      {
        "command": "ethical-json-tools.unstringify",
        "title": "Unstringify"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "jsonTools",
          "name": "JSON Tools",
          "icon": "json"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "jsonTools",
        "contents": "[Validate](command:ethical-json-tools.validate) \n [Prettify](command:ethical-json-tools.prettify) \n [Minify](command:ethical-json-tools.minify) \n[Stringify](command:ethical-json-tools.stringify) \n[Unstringify](command:ethical-json-tools.unstringify)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}